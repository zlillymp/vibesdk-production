		"react-hook-form": "^7.62.0",
		"react-markdown": "^10.1.0",
		"react-resizable-panels": "^3.0.6",
		"react-router": "^7.8.2",
		"recharts": "^3.2.1",
		"rehype-external-links": "^3.0.0",
		"remark-gfm": "^4.0.1",
		"sonner": "^2.0.7",
